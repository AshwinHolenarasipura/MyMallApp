                    android:id="@+id/textView5" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@+id/textView5"
                    android:layout_below="@+id/textView5"
                    android:text="Burger,FastFood"
                    android:textColor="@color/black"
                    android:textSize="10sp"
                    android:id="@+id/textView6" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@+id/textView6"
                    android:layout_below="@+id/textView6"
                    android:layout_marginTop="7dp"
                    android:text="TYPE"
                    android:textSize="10sp"
                    android:id="@+id/textView7"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@+id/textView8"
                    android:layout_below="@+id/textView8"
                    android:layout_marginTop="7dp"
                    android:text="AVERAGE COST"
                    android:textSize="10sp"
                    android:id="@+id/textView9" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@+id/textView7"
                    android:layout_below="@+id/textView7"
                    android:text="Quick Bites"
                    android:textColor="@color/black"
                    android:textSize="10sp"
                    android:id="@+id/textView8"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@+id/textView9"
                    android:layout_below="@+id/textView9"
                    android:text="Rs.500 for two (approx)"
                    android:textColor="@color/black"
                    android:textSize="10sp"
                    android:id="@+id/price"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Cash and Cards accepted"
                    android:textSize="10sp"
                    android:layout_below="@id/price"
                    android:layout_marginTop="5dp"
                    android:layout_marginStart="8dp"/>
                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/calltxt"
                    android:layout_alignBottom="@+id/calltxt"
                    android:layout_alignStart="@+id/textView12"
                    android:text="MORE INFO"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_below="@+id/textView11"
                    android:layout_marginEnd="14dp"
                    android:text="No Alcohol Available"
                    android:textColor="@color/black"
                    android:textSize="10sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignStart="@+id/textView12"
                    android:layout_below="@+id/textView12"
                    android:text="Veg and Non-veg"
                    android:textColor="@color/black"
                    android:textSize="10sp" />

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="180dp"
                android:id="@+id/openinghours"
                android:layout_below="@id/quickinfo"
                android:layout_marginTop="10dp"
                android:background="@color/white">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="11dp"
                    android:text="Opening Hours"
