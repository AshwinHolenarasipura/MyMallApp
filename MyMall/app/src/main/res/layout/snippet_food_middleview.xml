                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:id="@+id/lin4"
                    android:layout_below="@id/lin3"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">
