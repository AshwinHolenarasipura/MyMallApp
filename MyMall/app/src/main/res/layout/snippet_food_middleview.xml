                        android:textSize="9sp"
                        android:id="@+id/callminitxt"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="55dp"
                        android:layout_marginTop="8dp"
                        android:text="Been Here"
                        android:textColor="@color/black"
                        android:textSize="9sp"
                        android:id="@+id/Beenthereminitxt"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="45dp"
                        android:layout_marginTop="8dp"
                        android:text="Bookmark"
                        android:textColor="@color/black"
                        android:textSize="9sp"
                        android:id="@+id/Bookmarkminitxt"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="41dp"
                        android:layout_marginTop="8dp"
                        android:text="Avg Rating"
                        android:textColor="@color/black"
                        android:textSize="9sp"
                        android:id="@+id/avgratingminitxt"/>
                </LinearLayout>
            </RelativeLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_below="@id/foodinfo"
                android:layout_marginTop="10dp"
                android:background="@color/white"
                android:id="@+id/relmenu">

