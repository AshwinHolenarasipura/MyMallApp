                    android:textSize="13sp"
                    android:id="@+id/openingtxt"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:id="@id/lin1"
                    android:layout_below="@id/openingtxt"
