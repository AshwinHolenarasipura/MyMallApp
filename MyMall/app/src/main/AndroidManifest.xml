<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.mymall">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MapActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".DisplayStoryActivity" />
        <activity android:name=".MovieActivity" />
        <activity android:name=".FoodActivity" />
        <activity android:name=".ShoppingActivity" />
        <activity android:name=".EntertainmentActivity" />
        <activity android:name=".ServiceActivity" />
        <activity android:name=".PhotoDisplayActivity" />
        <activity android:name=".MovieInfoActivity" />
        <activity android:name=".MallinfoActivity" />
        <activity android:name=".WebPageDisplayActivity" />
        <activity android:name=".CategoryShoppingDisplay"></activity>
    </application>

</manifest>